{
  "regions":["bru"],
  "build":{
    "env": {
      "GRAPHQL_ENDPOINT":"https://api.fwrlines.com/graphql"
    }
  },
  "env":{
    "BACKEND":"true"
  }
  "headers": [
    {
      "source": "/.*",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "max-age=3600, s-maxage=3600, stale-while-revalidate"
        }
      ]
    },
  ],
  "rewrites": [
    {
      "source": "/(?<fn>[A-Za-z0-9-\/]+).(?<ext>(png|jpg|woff|woff2|otf|eot|ttf|ico|svg|glb|xml|txt))",
      "destination": "/$fn.$ext",
    },
    {
		  "source":"/(?<fn>[A-Za-z0-9~.-\/]+).(?<ext>(js|css))",
			"destination": "/$fn.$ext",
    },
  	{
      "source": "/.*",
      "destination": "/public/index.html",
  	}
  ],
}
