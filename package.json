  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client:dev:start": "webpack-dev-server --progress --config ./webpack.client.dev.js --watch",
    "client:dev:compile": "webpack --config ./webpack.client.dev.js",
    "client:prod:start": "webpack-dev-server --progress --config ./webpack.client.prod.js --watch --info-verbosity verbose",
    "client:prod:start:bugged": "webpack-dev-server --progress --config ./webpack.client.prod.js --watch --info-verbosity verbose",
    "client:prod:compile": "webpack --config ./webpack.client.prod.js",
    "cds": "npm run client:dev:start",
    "cdc": "npm run client:dev:compile",
    "cdcw": "npm run cdc -- -- --watch",
    "cps": "npm run client:prod:start",
    "cpc": "npm run client:prod:compile",
    "cpcw": "npm run cpc -- -- --watch",
    "build": "npm run cpc",
    "deploy-commit": "git add . && git commit -m '[Auto] Compile and deploy from cli' && git push",
    "bd": "npm version patch && npm run deploy-commit",
    "test-eslint": "eslint src/client.js"
  },
